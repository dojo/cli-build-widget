{
  "name": "@dojo/cli-build-widget",
  "version": "7.0.0-pre",
  "description": "CLI command to build Dojo custom elements",
  "engines": {
    "npm": ">=3.0.0"
  },
  "private": true,
  "homepage": "http://dojo.io",
  "bugs": {
    "url": "https://github.com/dojo/cli-build-widget/issues"
  },
  "license": "BSD-3-Clause",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dojo/cli-build-widget.git"
  },
  "scripts": {
    "artifact:build": "npm run artifact:build:elements && npm run artifact:build:lib && npm run artifact:build:lib:app",
    "artifact:build:elements": "cd test-app && npm run build:dist:evergreen && npm run build:dist && npm run build:dev:evergreen && npm run build:dev && cd ..",
    "artifact:build:lib": "cd test-app && npm run build:lib:dist:evergreen && npm run build:lib:dist && npm run build:lib:dev:evergreen && npm run build:lib:dev && cd ..",
    "artifact:build:lib:app": "cpx \"test-app/output/*-lib*/**\" test-lib-app/widget-lib-build && cd test-lib-app && npm run build && cd ..",
    "artifact:install": "npm run artifact:install:widgets && npm run artifact:install:lib:app",
    "artifact:install:widgets": "cd test-app && shx rm -rf node_modules && npm i && npm run install-build-widget",
    "artifact:install:lib:app": "cd test-lib-app && shx rm -rf node_modules && npm i",
    "artifact:package": "cd dist && npm pack -q ./release && shx mv dojo-cli-build-widget-* dojo-cli-build-widget.tgz && cd ..",
    "build:static:dev": "cpx \"{src,tests}/**/*.{d.ts,html,md,json,js,css}\" dist/dev",
    "build:static:release": "cpx \"src/**/*.{d.ts,json,js}\" dist/release",
    "build:cjs": "tsc",
    "build": "npm-run-all -p build:** -s dojo-package",
    "clean": "rimraf dist coverage test-app/output test-app/node_modules",
    "dojo-package": "dojo-package",
    "dojo-release": "dojo-release",
    "intern": "intern",
    "lint:tslint": "tslint -p .",
    "lint:prettier": "prettier -l \"{src,tests}/**/*.{ts,tsx}\"",
    "lint": "run-p lint:*",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,tests}/**/*.{ts,tsx}\"",
    "release": "run-s lint clean build \"dojo-release -- {@}\" --",
    "serve": "serve -p 9999",
    "test:ci": "run-p --race serve test",
    "test:cypress": "cypress run",
    "test": "run-s clean build artifact:package artifact:install intern artifact:build test:cypress",
    "uploadCoverage": "codecov --file=coverage/coverage.json",
    "watch:ts": "dojo-tsc-watcher -p tsconfig.json -- dojo-package",
    "watch": "run-p watch:ts \"build:static:** -- --watch\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@dojo/cli": "7.0.0-alpha.1",
    "@dojo/scripts": "~4.0.2",
    "@types/clean-webpack-plugin": "0.1.2",
    "@types/execa": "0.8.0",
    "@types/express": "4.16.1",
    "@types/globby": "6.1.0",
    "@types/gzip-size": "4.0.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jsonfile": "4.0.1",
    "@types/loader-utils": "1.1.3",
    "@types/log-symbols": "2.0.0",
    "@types/log-update": "2.0.0",
    "@types/mini-css-extract-plugin": "0.2.0",
    "@types/mockery": "1.4.29",
    "@types/node": "~9.6.5",
    "@types/optimize-css-assets-webpack-plugin": "1.3.1",
    "@types/ora": "1.3.1",
    "@types/rimraf": "2.0.2",
    "@types/sinon": "~4.3.3",
    "@types/strip-ansi": "3.0.0",
    "@types/tapable": "1.0.4",
    "@types/webpack": "^4.4.24",
    "@types/webpack-chunk-hash": "0.4.2",
    "@types/yargs": "10.0.0",
    "ajv": "6.7.0",
    "codecov": "~3.0.4",
    "cpx": "~1.5.0",
    "cypress": "3.1.3",
    "execa": "0.8.0",
    "husky": "^0.14.3",
    "lint-staged": "9.2.1",
    "mockery": "2.1.0",
    "npm-run-all": "~4.1.3",
    "prettier": "1.8.2",
    "rimraf": "2.6.2",
    "serve": "11.0.2",
    "shelljs": "0.7.8",
    "shx": "0.2.2",
    "sinon": "~4.5.0",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@dojo/framework": "7.0.0-alpha.2",
    "@dojo/webpack-contrib": "7.0.0-alpha.2",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "1.0.0",
    "cli-columns": "3.1.2",
    "css-loader": "1.0.1",
    "express": "4.16.2",
    "file-loader": "^3.0.1",
    "globby": "7.1.1",
    "gzip-size": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "json-css-module-loader": "1.0.2",
    "jsonfile": "4.0.0",
    "loader-utils": "1.1.0",
    "log-symbols": "2.1.0",
    "log-update": "2.3.0",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "ora": "1.3.0",
    "pkg-dir": "2.0.0",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "5.3.0",
    "slash": "1.0.0",
    "source-map-loader-cli": "0.0.1",
    "strip-ansi": "4.0.0",
    "terser-webpack-plugin": "1.1.0",
    "ts-loader": "5.4.5",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.4",
    "typed-css-modules": "0.3.1",
    "umd-compat-loader": "2.1.1",
    "webpack": "4.25.1",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-chunk-hash": "0.6.0",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-mild-compile": "3.3.1"
  }
}
